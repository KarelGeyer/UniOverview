<UserControl x:Class="UniOverview.Views.ContentWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:gradesView="clr-namespace:UniOverview.Views.GradesViews"
             xmlns:materialsView="clr-namespace:UniOverview.Views.MaterialsViews"
             xmlns:subjectViews="clr-namespace:UniOverview.Views.SubjectsViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <gradesView:GradesView />
        </StackPanel>
        <StackPanel>
            <materialsView:MaterialsView />
        </StackPanel>
        <StackPanel>
            <subjectViews:SubjectsView />
        </StackPanel>
    </Grid>
</UserControl>
